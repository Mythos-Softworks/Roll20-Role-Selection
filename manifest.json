{
  "name": "Roll20 Role Selection",
  "manifest_version": 2,
  "version": "1.0",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "https://app.roll20.net/editor/setcampaign/*"
  ],
  "background": {
    "scripts": [
      "redirect.js"
    ],
    "persistent": true
  },
  "icons": {
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon128.png"
  }
}